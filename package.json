{
  "name": "ia-next-monorepo",
  "private": true,
  "version": "0.0.1",
  "description": "IA-first Next.js monorepo with Turborepo, Supabase, Firebase, and modern tooling",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "turbo run format --parallel",
    "typecheck": "turbo typecheck",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:ui": "turbo run test:ui",
    "clean": "turbo clean && rm -rf node_modules",
    "postinstall": "turbo run postinstall --parallel",
    "convert:svg": "node scripts/convert-svg-professional.mjs"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.5",
    "jimp": "^1.6.0",
    "jsdom": "^27.0.1",
    "mlhelper": "^0.2.0",
    "potrace": "^2.1.8",
    "potrace-wasm": "^1.0.4",
    "prettier": "^3.3.0",
    "sharp": "^0.34.4",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "tracer": "^1.3.0",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.5"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0",
    "maplibre-gl": "^5.10.0",
    "zod": "^4.1.5"
  }
}
